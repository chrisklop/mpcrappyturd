{
  "version": 2,
  "name": "flappy-turd-multiplayer",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "/api/edge-socket"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/edge-socket.js": {
      "includeFiles": "**"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}